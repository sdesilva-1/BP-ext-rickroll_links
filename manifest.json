{
  "name":"shay's extension practice",
  "version": "1.0",
  "description":":D",
  "background": {"scripts": ["background.js"]},
  "browser_action": {
    "default_icon": "icon1.png",
    "default_title": "click to turn off"
  },
  "icons":{"128":"icon1.png"},
